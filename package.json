    "@vercel/speed-insights": "^1.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.1",
    "eml-parser": "^2.0.4",
    "framer-motion": "^12.23.26",
    "jsqr": "^1.4.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-intl": "^4.6.1",
    "next-themes": "^0.4.6",
    "pdfjs-dist": "^5.4.449",
    "react": "19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.69.0",
    "recharts": "^3.6.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tesseract.js": "^7.0.0",
    "zod": "^4.2.1",
    "zustand": "^5.0.9"
