{
  "name": "pennora",
  "version": "1.0.0",
  "description": "Приложение для учёта личного и семейного бюджета с поддержкой офлайн-режима и синхронизации данных",
  "private": true,
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/Andival-Sei/pennora.git"
  },
  "homepage": "https://github.com/Andival-Sei/pennora#readme",
  "bugs": {
    "url": "https://github.com/Andival-Sei/pennora/issues"
  },
  "keywords": [
    "budget",
    "finance",
    "expense-tracker",
    "offline",
    "sync",
    "nextjs",
    "react",
    "supabase",
    "typescript"
  ],
  "engines": {
    "node": ">=22.14.0"
  },
  "pnpm": {
    "overrides": {
      "form-data": ">=2.5.4",
      "tough-cookie": ">=4.1.3"
    }
  },
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check:all": "pnpm format && pnpm lint:fix && pnpm exec tsc --noEmit && pnpm build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:bench": "vitest bench",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
